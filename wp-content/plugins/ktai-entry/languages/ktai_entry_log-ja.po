# ----- inject.php --------------------------------------------------
msgid "Unknown envelope sender"
msgstr "不明な表書き差出人"

msgid ""
"***************************\n"
"Received a %1$d-byte-message from %2$s"
msgstr ""
"***************************\n"
"%1$d バイトのメッセージが %2$s より届きました。"

# ----- retrieve.php --------------------------------------------------
msgid "Retrieval completed."
msgstr "取り込みが完了しました"

msgid "Your request could not be understood by the server due to malformed syntax."
msgstr "あなたのリクエストはサーバーに理解されませんでした。書式が正しくないためです。"

msgid "Retrieval interval does not elapsed."
msgstr "取り込み時間になっていません。"

msgid "The POP3 config is not valid."
msgstr "POP3 設定が正しくありません。"

msgid "There doesn't seem to be any new mail."
msgstr "新着メールはないようです。"

msgid ""
"***************************\n"
"There is %d message(s)."
msgstr ""
"***************************\n"
"%d 通のメッセージがあります。"

msgid "Error at #%1$d: %2$s"
msgstr "#%1$d でエラー: %2$s"

msgid "Can't delete message #%1$d: %2$s"
msgstr "メッセージ #%1$d を削除できません: %2$s"

msgid "Mission complete, message \"%d\" deleted."
msgstr "任務完了。メッセージ \"%d\" を削除しました。"

# ----- post.php --------------------------------------------------
msgid "Text %1$d bytes, Attachment %2$d part(s)"
msgstr "テキストが %1$d バイト、添付が %2$d 通"

msgid "Author  : %1$s (ID: %2$d)"
msgstr "作成者  : %1$s (ID: %2$d)"

msgid "Date    : "
msgstr "日時    : "

msgid "Date GMT: "
msgstr "日時 GMT: "

msgid "Title   : "
msgstr "タイトル: "

msgid "+-- Content -------------------"
msgstr "+-- 内容 ----------------------"

msgid "Inserted a post with ID: %1$d, status: %2$s"
msgstr "投稿を作成しました。ID:  %1$d, ステータス: %2$s"

msgid "Updated the new post to status \"%1$s\" with %2$d image(s)."
msgstr "新規投稿を %2$d 枚の画像つきとし、ステータスを \"%1$s\" に更新しました。"

msgid "(Unknown)"
msgstr "(不明)"

msgid "1 message from %1$s, Pictogram type: %2$s"
msgstr "1 通のメッセージが %1$s から届きました。絵文字形式: %2$s"

msgid "Category: %s"
msgstr "カテゴリー: %s"

msgid "Assign cats: \"%1$s\" -> %2$s"
msgstr "カテゴリー割り当て: \"%1$s\" → %2$s"

msgid "No categories found from: \"%s\""
msgstr "次の文字列に対するカテゴリーがありません: \"%s\""

msgid "Tags: \"%s\""
msgstr "タグ: \"%s\""

msgid "Rotation: %s"
msgstr "回転方向:\"%s\""

msgid "Post slug: \"%s\""
msgstr "投稿スラッグ: \"%s\""

msgid "Decide post date by image #%d"
msgstr "投稿日時を画像 #%d から決定。"

msgid "Post date: \"%s\""
msgstr "投稿日時: \"%s\""

msgid "Invalid DATE command: \"%s\""
msgstr "不正な DATE コマンド: \"%s\""

msgid "Status: %s"
msgstr "ステータス: %s"

msgid "(N/A)"
msgstr "(未定)"

msgid "Signature chopped at byte position: %d"
msgstr "署名を次のバイト位置から切り落しました: %d"

msgid "GD not available."
msgstr "GD が使えません。"

msgid "Saving file: %s"
msgstr "ファイルを保存中: %s"

msgid "Failed inserting attachment for post #%1$d: %2$s"
msgstr "投稿 #%1$d に添付ファイルを追加できませんでした: %2$s"

msgid "Inserted attachment: #%1$d for post #%2$d"
msgstr "投稿 #%2$d に添付ファイルを追加しました: #%1$d"

msgid "Replaced the filename into %s"
msgstr "ファイル名を次に変更しました: %s"

msgid "Invalid image resource for file: %s"
msgstr "次のファイルで画像リソースを作れませんでした: %s"

msgid "Can't create a file: %s"
msgstr "ファイルを作れませんでした: %s"

msgid "Can't write to file: %s"
msgstr "ファイルに書き込みできませんでした: %s"

msgid "Can't close the file: %s"
msgstr "ファイルを閉じられませんでした: %s"

msgid "Can't chmod the file: %s"
msgstr "ファイルのアクセス権を変更できませんでした: %s"

msgid "Invalid image type \"%1$s\" for file: %2$s"
msgstr "不正な画像タイプ \"%1$s\" が次のファイルにありました:  %2$s"

msgid "Image without rotation: %1$dx%2$d type:%3$s"
msgstr "回転しない画像: %1$dx%2$d タイプ:%3$s"

msgid "Image with rotation(%1$s): %2$dx%3$d type:%4$s"
msgstr "画像を回転(%1$s): %1$dx%2$d タイプ:%3$s"

msgid "Can't write rotated image: %s"
msgstr "回転した画像を書き込めません: %s"

msgid "EXIF functions not available."
msgstr "EXIF 関数が使えません。"

msgid "Post date \"%1$s\" by EXIF of image: %2$s"
msgstr "投稿日時 \"%1$s\" を次の画像の EXIF から読み出しました: %2$s"

msgid "Post date \"%1$s\" by the filename of image: %2$s"
msgstr "投稿日時 \"%1$s\" を次の画像のファイル名から読み出しました: %2$s"

# ----- encode.php --------------------------------------------------
msgid "Detect %1$s encoding as \"%2$s\""
msgstr "%1$s のエンコーディングを \"%2$s\" と検出しました。"

msgid "Invalid character found for %1$s encoding."
msgstr "%1$s エンコーディングに対して不正な文字がありました。"

msgid "Skipped %1$s/%2$s part."
msgstr "%1$s/%2$s パートをスキップしました。"

msgid "Detect text/%1$s part encoding as \"%2$s\""
msgstr "text/%1$s パートのエンコーディングを \"%2$s\" と検出しました。"

msgid "Found %1$s/%2$s part with filename: %3$s"
msgstr "%1$s/%2$s パートを見つけました。ファイル名: %3$s"

msgid "Invalid filename \"%1$s\" for mime type \"%2$s/%3$s\""
msgstr "MIME タイプ \"%2$s/%3$s\" で \"%1$s\" は不正なファイル名です。"

# ----- operators.php --------------------------------------------------
msgid "Pictogram encoding is ISO-2022-JP."
msgstr "絵文字のエンコーディングは ISO-2022-JP です。"

msgid "Pictogram encoding is Shift_JIS."
msgstr "絵文字のエンコーディングは Shift_JIS です。"

